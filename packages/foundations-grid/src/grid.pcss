/**
 * Desktop grid
 *
 * Columns number: 12 column(s)
 * Column width: 76px
 * Column space: 32px
 *
 * Max content width: 1264px
 * Max width: 1296px
 */
:root {
  --container: 81rem;
  --gutter: 1rem;
  --gutter-compensation: -1rem;
}

.coop-l-container {
  max-width: var(--container);
  padding-left: var(--gutter);
  padding-right: var(--gutter);

  @media (--mq-large) {
    margin: 0 calc(var(--gutter) / 2);
  }

  @media (--mq-xlarge) {
    margin: 0 auto;
  }
}

.coop-l-grid {
  width: calc(100% + var(--gutter));
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-left: -0.5rem;
  margin-right: -0.5rem;

  @media (--mq-xlarge) {
    width: calc(100% + (var(--gutter) * 2));
    margin-left: var(--gutter-compensation);
    margin-right: var(--gutter-compensation);
  }
}

.coop-l-grid__item {
  flex: 1 0 100%;
  flex-basis: 100%;
  max-width: 100%;
  padding-left: calc(var(--gutter) / 2);
  padding-right: calc(var(--gutter) / 2);

  @media (--mq-xlarge) {
    padding-left: var(--gutter);
    padding-right: var(--gutter);
  }
}

.coop-l-grid--collapse .coop-l-grid__item,
.coop-l-grid__item--collapse {
  padding-left: 0;
  padding-right: 0;
}

.coop-l-grid__item--1 {
  flex-basis: 8.33333333%;
  max-width: 8.33333333%;
}
.coop-l-grid__item--2 {
  flex-basis: 16.66666667%;
  max-width: 16.66666667%;
}
.coop-l-grid__item--3 {
  flex-basis: 25%;
  max-width: 25%;
}
