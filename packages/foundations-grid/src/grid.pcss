}
.coop-l-grid__item--5 {
  flex-basis: 41.66666667%;
  max-width: 41.66666667%;
}
.coop-l-grid__item--6 {
  flex-basis: 50%;
  max-width: 50%;
}
.coop-l-grid__item--7 {
  flex-basis: 58.33333333%;
  max-width: 58.33333333%;
}
.coop-l-grid__item--8 {
  flex-basis: 66.66666667%;
  max-width: 66.66666667%;
}
.coop-l-grid__item--9 {
  flex-basis: 75%;
