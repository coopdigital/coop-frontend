@import "../../vars/colors-cls.pcss";

/* Co-op Legal Services (Blue) */
.coop-u-brand-cls-blue-dark {
  color: var(--color-brand-cls-blue-dark);
}

.coop-u-brand-cls-blue-dark-bg {
  background: var(--color-brand-cls-blue-dark);
}

.coop-u-brand-cls-blue-dark-fill {
  fill: var(--color-brand-cls-blue-dark);
}

.coop-u-brand-cls-blue-dark-stroke {
  stroke: var(--color-brand-cls-blue-dark);
  border-color: var(--color-brand-cls-blue-dark);
}

.coop-u-brand-cls-blue-mid {
  color: var(--color-brand-cls-blue-mid);
}

.coop-u-brand-cls-blue-mid-bg {
