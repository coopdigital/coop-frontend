}

.coop-u-brand-cls-blue-mid-stroke {
  stroke: var(--color-brand-cls-blue-mid);
  border-color: var(--color-brand-cls-blue-mid);
}

.coop-u-brand-cls-blue-light {
  color: var(--color-brand-cls-blue-light);
}

.coop-u-brand-cls-blue-light-bg {
  background: var(--color-brand-cls-blue-light);
}

.coop-u-brand-cls-blue-light-fill {
  fill: var(--color-brand-cls-blue-light);
}

.coop-u-brand-cls-blue-light-stroke {
  stroke: var(--color-brand-cls-blue-light);
  border-color: var(--color-brand-cls-blue-light);
}

